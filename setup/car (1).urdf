<?xml version="1.0"?>
<robot name="race_car">
  <!-- Main car body - rectangular prism -->
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.15625"/>
      <inertia ixx="0.0015625" ixy="0" ixz="0" iyy="0.0015625" iyz="0" izz="0.00078125"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.25 0.125 0.075"/>
      </geometry>
      <material name="blue">
        <color rgba="0.2 0.2 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.25 0.125 0.075"/>
      </geometry>
    </collision>
  </link>

  <!-- Front Left Wheel -->
  <link name="front_left_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.0078125"/>
      <inertia ixx="0.000012207" ixy="0" ixz="0" iyy="0.000012207" iyz="0" izz="0.000012207"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.0375" length="0.025"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.0375" length="0.025"/>
      </geometry>
    </collision>
  </link>

  <joint name="front_left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_left_wheel"/>
    <origin xyz="0.0875 0.075 -0.03" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Front Right Wheel -->
  <link name="front_right_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.0078125"/>
      <inertia ixx="0.000012207" ixy="0" ixz="0" iyy="0.000012207" iyz="0" izz="0.000012207"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.0375" length="0.025"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.0375" length="0.025"/>
      </geometry>
    </collision>
  </link>

  <joint name="front_right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_right_wheel"/>
    <origin xyz="0.0875 -0.075 -0.03" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Rear Left Wheel -->
  <link name="rear_left_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.0078125"/>
      <inertia ixx="0.000012207" ixy="0" ixz="0" iyy="0.000012207" iyz="0" izz="0.000012207"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.0375" length="0.025"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.0375" length="0.025"/>
      </geometry>
    </collision>
  </link>

  <joint name="rear_left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rear_left_wheel"/>
    <origin xyz="-0.0875 0.075 -0.03" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Rear Right Wheel -->
  <link name="rear_right_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.0078125"/>
      <inertia ixx="0.000012207" ixy="0" ixz="0" iyy="0.000012207" iyz="0" izz="0.000012207"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.0375" length="0.025"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.0375" length="0.025"/>
      </geometry>
    </collision>
  </link>

  <joint name="rear_right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rear_right_wheel"/>
    <origin xyz="-0.0875 -0.075 -0.03" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>
</robot>
